    // Dance ...............................................................{{{1
    "dance.smartCase": true,
    "dance.modes": {
        "helix/normal": {
            "lineNumbers": "inherit",
            "cursorStyle": "block",
            // "cursorStyle": "line",
            "selectionBehavior": "character"
            // "selectionBehavior": "caret"
        },
        "insert": {
            "lineNumbers": "inherit",
            "cursorStyle": "line",
            "selectionBehavior": "character"
        },
        "input": {
            "inheritFrom": "insert"
        },
        "select": {
            "inheritFrom": "normal",
            "onLeaveMode": [
                [
                    ".selections.save",
                    {
                        "register": " ^",
                        "style": {
                            "borderColor": "$editor.selectionBackground",
                            "borderStyle": "solid",
                            "borderWidth": "2px",
                            "borderRadius": "1px"
                        },
                        "until": [
                            [
                                "mode-did-change",
                                {
                                    "include": [
                                        "normal",
                                        "select"
                                    ]
                                }
                            ],
                            [
                                "selections-did-change"
                            ]
                        ]
                    }
                ]
            ]
        }
    },
    "dance.menus": {
        "goto": {
            "title": "Goto",
            "items": {
                "g": {
